@using ContentModels = Umbraco.Web.PublishedModels;
@model Umbraco.Web.Models.ContentModel
@{
    Layout = "Master.cshtml";
    var slogan = Model.Content.Value<string>("slogan");
}



<!-- HOME -->
<section id="home" data-stellar-background-ratio="0.5">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">

            <div class="col-md-6 col-sm-12">
                <div class="home-info">
                    <h1>@slogan</h1>
                    <a href="#about" class="btn section-btn smoothScroll">Start a project</a>
                    <span>
                        CALL US (+66) 010-020-0340
                        <small>For any inquiry</small>
                    </span>
                </div>
            </div>

            <div class="col-md-6 col-sm-12">
                <div class="home-video">
                    <div class="embed-responsive embed-responsive-16by9">
                        <iframe src="https://www.youtube.com/embed/AqcjdkPMPJA" frameborder="0" allowfullscreen></iframe>
                    </div>
                </div>
            </div>

        </div>
    </div>
</section>